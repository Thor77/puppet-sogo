<%- | Hash $config | -%>
//
// Managed by Puppet
//

{
  <%- $config.each |$key, $value| { -%>
    <%- if $value =~ Hash { -%>
  <%= $key %> = (
    {
      <%- $value.each |$k, $v| { -%>
      <%= $k %> = <%= $v %>;
      <%- } -%>
    }
  );
    <%- } else { -%> 
  <%= $key %> = <%= $value %>; 
    <%- } -%>
  <%- } -%>
}
